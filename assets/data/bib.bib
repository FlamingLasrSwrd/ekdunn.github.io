@article{tang_nitric_2009,
	title = {Nitric oxide mediated intestinal injury is required for alcohol-induced gut leakiness and liver damage},
	volume = {33},
	issn = {0145-6008},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2950098/},
	doi = {10.1111/j.1530-0277.2009.00946.x},
	abstract = {Background
Alcoholic liver disease (ALD) requires endotoxemia and is commonly associated with intestinal barrier leakiness. Using monolayers of intestinal epithelial cells as an in vitro barrier model, we showed that ethanol-induced intestinal barrier disruption is mediated by iNOS (inducible nitric-oxide synthase) upregulation, NO (nitric oxide) overproduction, and oxidation/nitration of cytoskeletal proteins. We hypothesized that iNOS inhibitors (L-NAME, L-NIL) in vivo will inhibit the above cascade and liver injury in an animal model of alcoholic steatohepatitis (ASH).

Methods
Male Sprague-Dawley rats were gavaged daily with alcohol (6 g/kg/day) or dextrose for 10 weeks ± L-NAME, L-NIL or vehicle. Systemic and intestinal NO levels were measured by nitrites and nitrates in urine and tissue samples, oxidative damage to the intestinal mucosa by protein carbonyl and nitrotyrosine, intestinal permeability by urinary sugar tests, and liver injury by histological inflammation scores, liver fat, and myeloperoxidase activity.

Results
Alcohol caused tissue oxidation, gut leakiness, endotoxemia and ASH. L-NIL and L-NAME, but not the D-enantiomers, attenuated all steps in the alcohol-induced cascade including NO overproduction, oxidative tissue damage, gut leakiness, endotoxemia, hepatic inflammation and liver injury.

Conclusions
The mechanism we reported for alcohol-induced intestinal barrier disruption in vitro – NO overproduction, oxidative tissue damage, leaky gut, endotoxemia and liver injury – appears to be relevant in vivo in an animal model of alcohol-induced liver injury. That iNOS inhibitors attenuated all steps of this cascade suggests that prevention of this cascade in alcoholics will protect the liver against the injurious effects of chronic alcohol and that iNOS may be a useful target for prevention of ALD.},
	number = {7},
	urldate = {2017-01-17},
	journal = {Alcoholism, clinical and experimental research},
	author = {Tang, Yueming and Forsyth, Christopher B. and Farhadi, Ashkan and Rangan, Jayanthi and Jakate, Shriram and Shaikh, Maliha and Banan, Ali and Fields, Jeremy Z. and Keshavarzian, Ali},
	month = jul,
	year = {2009},
	pmid = {19389191},
	pmcid = {PMC2950098},
	pages = {1220--1230},
	annote = {
Feeding of alcohol to rats for 10 weeks significantly increased serum endotoxin levels 7 fold, from 0.08 ± 0.02 to 0.58 ± 0.11 EU/ml (Fig. 5, p{\textless}0.05). L-NAME reduced this increase by 46\%) to 0.35 ± 0.02 EU/ml in alcohol fed rats (Fig. 5, p{\textless}0.05); L-NIL reduced the increase by 84\% to 0.16 ± 0.02 EU/ml. Both agents thus attenuated alcohol-induced endotoxemia.


Feeding of alcohol to rats for 10 weeks increased MPO activity about 18 fold from 2.1 ± 0.03 to 38 ± 4.11 Units/mg tissue (Fig. 6a, p{\textless}0.05). iNOS inhibitors significantly decreased hepatic MPO activity in alcohol fed rats. For L-NIL, the inhibition was 92\% (to 5.1 ± 0.7 Units/mg tissue); for L-NAME, inhibition was 86\% (7.2 ± 0.6; Fig. 6a, p{\textless}0.05).
},
	annote = {
In contrast, iNOS appears to be the key source of the EtOH-induced increase in NO levels because, in alcohol fed rats, iNOS inhibition by L-NIL significantly decreased NO levels in both urinary and colonic mucosa. Both L-NAME and L-NIL decreased NO levels in the urine and in colonic mucosa of alcohol fed rats. However, the inhibitory effect of L-NIL was significantly greater than L-NAME (98\% vs 46\% for urine NO; 97\% vs 64\% for colonic mucosa NO; p{\textless}0.05, Fig. 1).
},
	annote = {
That iNOS inhibitors attenuated all steps of this cascade suggests that prevention of this cascade in alcoholics will protect the liver against the injurious effects of chronic alcohol and that iNOS may be a useful target for prevention of ALD.
},
	annote = {
Total NO production in urine was increased 8.5 fold from 81 ± 11 to 690 ± 75 μmol/mg (p{\textless}0.05) in EtOH fed rats compared with dextrose fed control rats (Fig. 1a).


The EtOH-induced increase in NO was inhibited in rats that were fed, in addition to EtOH, a nonselective iNOS inhibitor, L-NAME (46\% decrease to 410 ± 35, p{\textless}0.05, Fig. 1a) or a selective iNOS inhibitor, L-NIL (98\% decrease to 90 ± 8, p{\textless}0.05, Fig. 1a).


EtOH induced a 4.1 fold increase in NO production in colonic mucosa (from 850 ± 89 to 3548 ± 350 μmol/mg; p{\textless}0.05) (EtOH-fed rats compared with dextrose-fed control rats) (Fig. 1b). The EtOH-induced increase in colonic tissue NO was inhibited by a nonselective iNOS inhibitor, L-NAME (64\% decrease to 1800 ± 160, p{\textless}0.05, Fig. 1b) or a selective iNOS inhibitor, L-NIL (97\% decrease to 910 ± 98, p{\textless}0.05, Fig. 1b).
},
	file = {Tang_Forsyth_Farhadi_Rangan_Jakate_Shaikh_Banan_Fields_Keshavarzian_2009_Nitric oxide mediated intestinal injury is required for alcohol-induced gut.pdf:/media/truecrypt1/zotero//Alcoholism, clinical and experimental research/2009/Tang_Forsyth_Farhadi_Rangan_Jakate_Shaikh_Banan_Fields_Keshavarzian_2009_Nitric oxide mediated intestinal injury is required for alcohol-induced gut.pdf:application/pdf}
}

