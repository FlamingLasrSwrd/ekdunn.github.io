	journal = {Annu. Rev. Plant. Physiol.},
	author = {Growthl, Plant},
	year = {1952},
	note = {00000},
	pages = {347--362},
	file = {Growthl_1952_THE EFFECT OF TEMPERATURE ON PLANT GROWTHl By.pdf:/media/truecrypt1/zotero//Annu. Rev. Plant. Physiol./1952/Growthl_1952_THE EFFECT OF TEMPERATURE ON PLANT GROWTHl By.pdf:application/pdf}
}

