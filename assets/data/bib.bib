@article{jiao_l-glutamate_2015,
	title = {l-{Glutamate} {Enhances} {Barrier} and {Antioxidative} {Functions} in {Intestinal} {Porcine} {Epithelial} {Cells}},
	volume = {145},
	issn = {0022-3166, 1541-6100},
	url = {http://jn.nutrition.org/content/145/10/2258},
	doi = {10.3945/jn.115.217661},
	abstract = {Background: l-Glutamate (Glu) is a major amino acid in milk and postweaning diets for mammals (including pigs and human infants). However, effects of Glu on intestinal mucosal barrier and antioxidative functions are unknown.
Objective: This study tested the hypothesis that Glu may enhance the barrier function of intestinal porcine epithelial cell line 1 (IPEC-1) cells by upregulating the expression of tight junction proteins.
Methods: IPEC-1 cells were cultured with or without Glu in the presence or absence of 1 mmol/L diquat (an oxidant) for indicated time points. Cell numbers, transepithelial electrical resistance (TEER), mRNA, and protein abundance of glutamate transporter, the release of lactate dehydrogenase (LDH), and the abundance of tight junction proteins were determined.
Results: Compared with 0 mmol/L Glu, 0.5-, 1-, and 2 mmol/L Glu stimulated (P {\textless} 0.05) cell growth by 13–37\% at 24 h and 12–34\% at 48 h, respectively. In addition, 0.5 mmol/L Glu increased (P {\textless} 0.05) TEER (by 58\% at 24 h and by 98\% at 48 h, respectively). These effects of Glu were associated with increased mRNA abundance of Glu transporter solute carrier family 1 member 1 (SLC1A1) by 30–130\% and protein abundance of excitatory amino acid transporter 3 (encoded by SLC1A1) by 19–34\%, respectively. In a cell model of oxidative stress induced by 1 mmol/L diquat, 0.5 mmol/L Glu enhanced cell viability, TEER, and membrane integrity (as indicated by the reduced release of LDH) in IPEC-1 cells by increasing the abundance of the tight junction proteins occludin, claudin-3, zonula occludens (ZO)-2, and ZO-3.
Conclusion: These findings indicate that Glu plays an important role in mucosal barrier function by enhancing cell growth and maintaining membrane integrity in response to oxidative stress.},
	language = {en},
	number = {10},
	urldate = {2017-01-12},
	journal = {The Journal of Nutrition},
	author = {Jiao, Ning and Wu, Zhenlong and Ji, Yun and Wang, Bin and Dai, Zhaolai and Wu, Guoyao},
	month = oct,
	year = {2015},
	pmid = {26338884},
	keywords = {antioxidative function, glutamate, intestinal epithelial cell, intestinal–mucosal barrier, oxidative stress},
	pages = {2258--2264},
	file = {Jiao_Wu_Ji_Wang_Dai_Wu_2015_l-Glutamate Enhances Barrier and Antioxidative Functions in Intestinal Porcine.pdf:/media/truecrypt1/zotero//The Journal of Nutrition/2015/Jiao_Wu_Ji_Wang_Dai_Wu_2015_l-Glutamate Enhances Barrier and Antioxidative Functions in Intestinal Porcine.pdf:application/pdf;Snapshot:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/2F9IF7F9/2258.html:text/html}
}

@article{roth_regulative_2002,
	title = {Regulative potential of glutamine--relation to glutathione metabolism},
	volume = {18},
	issn = {0899-9007},
	abstract = {Glutamine (GLN) is the most abundant free amino acid (AA) in the human body. Under GLN-free conditions, which can be obtained when cells are cultivated in vitro, tissue cells cannot grow. Therefore, when classifying GLN as a "non-essential" AA, one must consider that in the human body GLN is synthesized from essential AAs and is continuously delivered from skeletal muscle to other organs. It is fascinating that a relatively simple AA like GLN can stimulate a large variety of cellular reactions. GLN stimulates not only the growth of cells but also the expression of surface antigens, the formation of cytokines, and the synthesis of heat shock proteins. Further, a GLN deficiency leads to a cell cycle arrest in G(0) to G(1) and reduces apoptosis. Interestingly, many of these biological activities also are associated with the cellular reduced oxygen potential, which depends mainly on the ratio of reduced to oxidized glutathione. Experimental animal studies have shown that the administration of GLN increases tissue concentrations of reduced glutathione. This review describes the relation of GLN to reduced glutathione metabolism and discusses the alteration of reduced glutathione metabolism under a variety of clinical conditions such as reperfusion injury, myocardial infarction, respiratory insufficiency, cancer, diabetes, liver disease, and clinical protein catabolism.},
	language = {eng},
	number = {3},
	journal = {Nutrition (Burbank, Los Angeles County, Calif.)},
	author = {Roth, Erich and Oehler, Rudolf and Manhart, Nicole and Exner, Ruth and Wessner, Barbara and Strasser, Eva and Spittler, Andreas},
	month = mar,
	year = {2002},
	pmid = {11882392},
	keywords = {Apoptosis, Cell Cycle, Cytokines, Glutamine, Glutathione, Heat-Shock Proteins, Humans, Liver, Lymphocytes, Monocytes, Muscle, Skeletal},
	pages = {217--221}
}

@article{rohsenow_acute_2007,
	title = {The {Acute} {Hangover} {Scale}: {A} {New} {Measure} of {Immediate} {Hangover} {Symptoms}},
	volume = {32},
	issn = {0306-4603},
	shorttitle = {The {Acute} {Hangover} {Scale}},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2853365/},
	doi = {10.1016/j.addbeh.2006.10.001},
	abstract = {Purpose
No psychometrically established measure of acute hangover symptoms is published and available to use in experimental investigations. The present investigation combined data across three studies of residual alcohol effects to establish the properties of a new Acute Hangover Scale (AHS) based on symptoms supported in previous lab studies.

Methods
Professional mariners from a Swedish maritime academy (n = 54) and young adult students/recent graduates of urban U.S. universities (n = 135) participated in one of three within-subjects' studies of residual effects of heavy drinking (M = .114 g\% breath alcohol concentration [BrAC]). All drank placebo one evening and alcoholic drinks another evening followed by an 8-hour sleep period before completing the AHS 10-20 min after awakening.

Results
The AHS showed excellent internal consistency reliability the morning after alcohol. The AHS mean score and each item were significantly affected by beverage but not demographics or typical drinking, supporting validity.

Conclusions
The AHS is a reliable and valid instrument for assessing acute hangover symptoms in experimental investigations of residual alcohol effects.},
	number = {6},
	urldate = {2017-01-09},
	journal = {Addictive behaviors},
	author = {Rohsenow, Damaris J. and Howland, Jonathan and Minsky, Sara J. and Greece, Jacey and Almeida, Alissa and Roehrs, Timothy A.},
	month = jun,
	year = {2007},
	pmid = {17097819},
	pmcid = {PMC2853365},
	pages = {1314--1320},
	annote = {
Limitations of this study included alcohol administration only to about .12 g\%, and limited populations.
},
	annote = {
The AHS total (mean) score is valid as it was significantly affected by previous evening's consumption of alcohol versus placebo yet was not affected by demographic variables or recent drinking history.
},
	annote = {
The items “Thirsty” and “Tired” had the highest values, the only items scored in the “Moderate” range the morning after drinking alcohol. However, these items also were highest the morning after placebo administration so inspecting values only after alcohol may be misleading; people may be tired and thirsty at that time in the morning anyway.


The largest effect sizes as a function of alcohol ingestion were for “hangover”, “thirsty”, and “headache”, in that order. The other items all had medium or just below medium statistical effect sizes.
},
	file = {PubMed Central, Table 2\: Addict Behav. 2007 Jun\; 32(6)\: 1314–1320. Published online 2006 Nov 13. doi\:  10.1016/j.addbeh.2006.10.001:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/9KIWHH89/T2.html:text/html;Rohsenow_Howland_Minsky_Greece_Almeida_Roehrs_2007_The Acute Hangover Scale.pdf:/media/truecrypt1/zotero//Addictive behaviors/2007/Rohsenow_Howland_Minsky_Greece_Almeida_Roehrs_2007_The Acute Hangover Scale.pdf:application/pdf}
}

