@article{sachan_acetylcarnitine_1994,
	title = {Acetylcarnitine {Inhibits} {Alcohol} {Dehydrogenase}},
	volume = {203},
	issn = {0006-291X},
	url = {http://www.sciencedirect.com/science/article/pii/S0006291X84723540},
	doi = {10.1006/bbrc.1994.2354},
	abstract = {Carnitine and acetylcarnitine are used as dietary supplements and as therapeutic agents. Carnitine attenuates ethanol metabolism in intact animals but the in vitro activities of alcohol dehydrogenase (ADH), microsomal ethanol oxidizing system (MEOS) or catalase are not significantly altered by carnitine. Since acetylcarnitine was a far more potent inhibitor of ethanol oxidation than carnitine in hepatocytes, the activities of rat liver ADH and MEOS were determined with or without acetylcarnitine. The activity of ADH, not MEOS, was significantly inhibited by acetylcarnitine at NAD : acetylcarnitine ≤ 1. The inhibition is of a competitive nature where acetylcarnitine competes with NAD+ (Ki = 135 μmol.L−1). This finding is unique in that this is the first report of this function of acetylcarnitine and it is a novel interaction between two important nutrients, niacin and carnitine.},
	number = {3},
	urldate = {2017-06-23},
	journal = {Biochemical and Biophysical Research Communications},
	author = {Sachan, D. S. and Cha, Y. S.},
	month = sep,
	year = {1994},
	pages = {1496--1501},
	file = {ScienceDirect Snapshot:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/T7KGDWIU/S0006291X84723540.html:text/html}
}

