	file = {Howland_Rohsenow_Greece_Littlefield_Almeida_Heeren_Winter_Bliss_Hunt_Hermos_2010_The Effects of Binge Drinking on College Students’ Next-Day Academic.pdf:/media/truecrypt1/zotero//Addiction (Abingdon, England)/2010/Howland_Rohsenow_Greece_Littlefield_Almeida_Heeren_Winter_Bliss_Hunt_Hermos_2010_The Effects of Binge Drinking on College Students’ Next-Day Academic.pdf:application/pdf}
