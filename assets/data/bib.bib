@incollection{patel_chaga_2015,
	series = {Applied {Environmental} {Science} and {Engineering} for a {Sustainable} {Future}},
	title = {Chaga ({Inonotus} {Obliquus}) {Mushroom}: {Nutraceutical} {Assesement} {Based} on {Latest} {Findings}},
	copyright = {©2015 Springer International Publishing Switzerland},
	isbn = {978-3-319-12846-7 978-3-319-12847-4},
	shorttitle = {Chaga ({Inonotus} {Obliquus}) {Mushroom}},
	url = {http://link.springer.com/chapter/10.1007/978-3-319-12847-4_11},
	abstract = {Chaga mushroom (Inonotus obliquus) is a white-rot fungus growing on birch trees. It looks like burnt coal, has edibility and is regarded a traditional medicinal mushroom in China, Russia, Poland, other Baltic countries. It is hailed as a folk remedy against diabetes, digestive disorders, intestinal worms, liver ailments, cardiovascular diseases, tuberculosis and cancer etc. Recently, it has grabbed attention from mainstream medicine as a therapeutic mushroom. The discovery of a diverse range of secondary metabolites including, phenolic compounds, polysaccharides, melanins, lanosterol, inotodiol, ergosterol and trametenolic acid have promoted it as a potential drug candidate. Biological roles as antioxidant, antiinflammation, anticancer, immunomodulation, antiallergy, antilipemic, hypoglycaemic, antiviral, neurostimulation have been validated. This chapter delves into the biological roles and mechanisms governing the effects.},
	language = {en},
	urldate = {2017-03-28},
	booktitle = {Emerging {Bioresources} with {Nutraceutical} and {Pharmaceutical} {Prospects}},
	publisher = {Springer International Publishing},
	author = {Patel, Seema},
	year = {2015},
