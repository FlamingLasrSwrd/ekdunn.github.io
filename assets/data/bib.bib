@article{flanagan_role_2010,
	title = {Role of carnitine in disease},
	volume = {7},
	issn = {1743-7075},
	url = {https://doi.org/10.1186/1743-7075-7-30},
	doi = {10.1186/1743-7075-7-30},
	abstract = {Carnitine is a conditionally essential nutrient that plays a vital role in energy production and fatty acid metabolism. Vegetarians possess a greater bioavailability than meat eaters. Distinct deficiencies arise either from genetic mutation of carnitine transporters or in association with other disorders such as liver or kidney disease. Carnitine deficiency occurs in aberrations of carnitine regulation in disorders such as diabetes, sepsis, cardiomyopathy, malnutrition, cirrhosis, endocrine disorders and with aging. Nutritional supplementation of L-carnitine, the biologically active form of carnitine, is ameliorative for uremic patients, and can improve nerve conduction, neuropathic pain and immune function in diabetes patients while it is life-saving for patients suffering primary carnitine deficiency. Clinical application of carnitine holds much promise in a range of neural disorders such as Alzheimer's disease, hepatic encephalopathy and other painful neuropathies. Topical application in dry eye offers osmoprotection and modulates immune and inflammatory responses. Carnitine has been recognized as a nutritional supplement in cardiovascular disease and there is increasing evidence that carnitine supplementation may be beneficial in treating obesity, improving glucose intolerance and total energy expenditure.},
	urldate = {2017-07-21},
	journal = {Nutrition \& Metabolism},
	author = {Flanagan, Judith L. and Simmons, Peter A. and Vehige, Joseph and Willcox, Mark DP and Garrett, Qian},
	month = apr,
	year = {2010},
	pages = {30},
	annote = {Pages 30 in PDF},
	file = {Flanagan_Simmons_Vehige_Willcox_Garrett_2010_Role of carnitine in disease.pdf:/media/truecrypt1/zotero//Nutrition & Metabolism/2010/Flanagan_Simmons_Vehige_Willcox_Garrett_2010_Role of carnitine in disease.pdf:application/pdf;Snapshot:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/7QDUSDQQ/1743-7075-7-30.html:text/html}
}

