	year = {2014},
	note = {00000},
	keywords = {high resolution images, research, risks management, sustainable reconstruction},
	pages = {1--5},
	file = {Name et al_2014_Thermal Energy Storage Materials (PCMs) for Textile Application.pdf:/media/truecrypt1/zotero//Igarss 2014/2014/Name et al_2014_Thermal Energy Storage Materials (PCMs) for Textile Application.pdf:application/pdf}
