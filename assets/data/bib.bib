	file = {Rohsenow_Howland_Alvarez_Nelson_Langlois_Verster_Sherrard_Arnedt_2014_Effects of Caffeinated vs.pdf:/media/truecrypt1/zotero//Addictive behaviors/2014/Rohsenow_Howland_Alvarez_Nelson_Langlois_Verster_Sherrard_Arnedt_2014_Effects of Caffeinated vs.pdf:application/pdf}
