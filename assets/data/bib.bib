	journal = {Addictive behaviors},
	author = {Rohsenow, Damaris J. and Howland, Jonathan and Alvarez, Luisa and Nelson, Kerrie and Langlois, Breanne and Verster, Joris C. and Sherrard, Heather and Arnedt, J. Todd},
	month = jan,
