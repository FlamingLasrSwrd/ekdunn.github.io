	urldate = {2017-06-15},
	journal = {Diabetologia},
	author = {Beulens, J. W. J. and van Loon, L. J. C. and Kok, F. J. and Pelsers, M. and Bobbert, T. and Spranger, J. and Helander, A. and Hendriks, H. F. J.},
	month = jul,
	year = {2007},
	pmid = {17492425},
	pmcid = {PMC1914282},
	pages = {1388--1392},
	file = {Beulens et al_2007_The effect of moderate alcohol consumption on adiponectin oligomers and muscle.pdf:/media/truecrypt1/zotero//Diabetologia/2007/Beulens et al_2007_The effect of moderate alcohol consumption on adiponectin oligomers and muscle.pdf:application/pdf}
}

