	file = {Powell et al_2010_Mechanism for Prevention of Alcohol-Induced Liver Injury by Dietary Methyl.pdf:/media/truecrypt1/zotero//Toxicological Sciences/2010/Powell et al_2010_Mechanism for Prevention of Alcohol-Induced Liver Injury by Dietary Methyl.pdf:application/pdf}
