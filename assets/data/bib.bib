@article{szabo_alcohols_2015,
	title = {Alcohol’s {Effect} on {Host} {Defense}},
	volume = {37},
	issn = {2168-3492},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4590613/},
	abstract = {Alcohol affects many organs, including the immune system, with even moderate amounts of alcohol influencing immune responses. Although alcohol can alter the actions of all cell populations involved in the innate and adaptive immune responses, the effect in many cases is a subclinical immunosuppression that becomes clinically relevant only after a secondary insult (e.g., bacterial or viral infection or other tissue damage). Alcohol’s specific effects on the innate immune system depend on the pattern of alcohol exposure, with acute alcohol inhibiting and chronic alcohol accelerating inflammatory responses. The proinflammatory effects of chronic alcohol play a major role in the pathogenesis of alcoholic liver disease and pancreatitis, but also affect numerous other organs and tissues. In addition to promoting proinflammatory immune responses, alcohol also impairs anti-inflammatory cytokines. Chronic alcohol exposure also interferes with the normal functioning of all aspects of the adaptive immune response, including both cell-mediated and humoral responses. All of these effects enhance the susceptibility of chronic alcoholics to viral and bacterial infections and to sterile inflammation.},
	number = {2},
	urldate = {2017-01-25},
	journal = {Alcohol Research : Current Reviews},
	author = {Szabo, Gyongyi and Saha, Banishree},
	year = {2015},
	pmid = {26695755},
	pmcid = {PMC4590613},
	pages = {159--170},
	annote = {
Even a single episode of binge drinking can have measurable effects on the innate immune system, inducing a transient proinflammatory state within the first 20 minutes after alcohol ingestion, followed by an anti-inflammatory state 2 to 5 hours after alcohol ingestion (Afshar et al. 2015).
},
	annote = {
Furthermore, inflammatory reactions can occur in the brain after alcohol use, even in the absence of detectable LPS in the brain (Lippai et al. 2013a; Szabo and Lippai 2014). These observations suggest that although gut-derived LPS can promote tissue inflammation, another alcohol-induced component is required as well.
},
	annote = {
Specific aspects of the adaptive immune response also are affected. For example, even a single dose of alcohol may impair antigen-specific T-cell activation.
},
	annote = {
Thus, whereas acute alcohol exposure increases both IL-10 and TGFβ production in monocytes and macrophages, chronic alcohol exposure mostly is associated with decreased IL-10 production or prevents appropriate increases in IL-10 levels to counterbalance the overproduction of proinflammatory cytokines
},
	file = {Szabo_Saha_2015_Alcohol’s Effect on Host Defense.pdf:/media/truecrypt1/zotero//Alcohol Research  Current Reviews/2015/Szabo_Saha_2015_Alcohol’s Effect on Host Defense.pdf:application/pdf}
}

@article{calabrese_effects_2001,
	title = {Effects of {Acetyl}-{L}-{Carnitine} on the {Formation} of {Fatty} {Acid} {Ethyl} {Esters} in {Brain} and {Peripheral} {Organs} after {Short}-{Term} {Ethanol} {Administration} in {Rat}},
	volume = {26},
	issn = {0364-3190, 1573-6903},
	url = {https://link.springer.com/article/10.1023/A:1011003030287},
	doi = {10.1023/A:1011003030287},
	abstract = {Increasing evidence suggests that Fatty acid ethyl esters (FAEE) play a central role in ethanol induced organ damage. In the current study we measured FAEE formation in rats after short-term oral administration of ethanol, in the presence and absence of pre-treatment with acetyl-L-carnitine. Ethanol treatment caused a significant increase in the levels of FAEE, particularly in the brain and heart, but also in the kidney and liver. Increases in FAEE were associated with a significant increase in FAEE synthase activity, GSH transferase activity, and lipid hydroperoxide levels. Pre-treatment with acetyl-L-carnitine resulted in a significant reduction of FAEE accumulation, decrease in FAEE synthase and GSH transferase activities, and lipid hydroperoxide levels. Administration of acetyl-L-carnitine greatly reduced the metabolic abnormalities due to non-oxidative ethanol metabolism, through an increment in lipid metabolism/turnover and by the modulation of the activities of enzymes associated with FAEE synthesis. These results suggest a potentially important pharmacological role for acetyl-L-carnitine in the prevention of alcohol-induced cellular damage.},
	language = {en},
	number = {2},
	urldate = {2017-06-23},
	journal = {Neurochemical Research},
	author = {Calabrese, V. and Scapagnini, G. and Catalano, C. and Dinotta, F. and Bates, T. E. and Calvani, M. and Stella, A. M. Giuffrida},
	month = feb,
	year = {2001},
	pages = {167--174},
	annote = {Extracted Annotations (7/21/2017, 1:39:28 PM)
"In the current study we measured FAEE formation in rats after short-term oral administration of ethanol, in the presence and absence of pre-treatment with acetyl-L-carnitine." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
"Pretreatment with acetyl-L-carnitine resulted in a significant reduction of FAEE accumulation, decrease in FAEE synthase and GSH transferase activities, and lipid hydroperoxide levels." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
"These results suggest a potentially important pharmacological role for acetyl-L-carnitine in the prevention of alcohol-induced cellular damage." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
"The molecular pathology of ethanol is almost certainly multi-factorial, but one of its major effects is to alter the composition of" (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
The molecular pathology of ethanol is almost certainly multi-factorial, but one of its major effects is to alter the composition of lipids in various cells throughout the body. (note on p.167)
 
"However, it is not clear whether these alterations are related to any pharmacological effects of ethanol, per se, or to its oxidative metabolism, which produces oxidants and free radical-mediated reactions." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
"However, this oxidative metabolism of ethanol cannot account for cellular damage in organs such as brain, heart or pancreas, as this metabolic pathway is minimal or absent (1) and these tissues are free of substantial acetaldehyde production (2)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:167)
"Fatty acid ethyl ester synthase (FAEES) activity has been recognized in different organs, such as human heart, brain, pancreas, and liver and has been demonstrated to be a glutathione (GSH) transferase (5)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"One of the most prominent feature of FAEE formation is that these compounds may accumulate in concentrations as high as 100 μM in the myocardium, and that their half-life is approximately 16 hours; thus, they may persist at potentially toxic levels, long after ethanol has been fully cleared from the blood (7)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"FAEE are largely associated with mitochondrial membranes, where a lipase exists that cleaves FAEE to free fatty acids, which are also known uncouplers of oxidative phosphorylation;" (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"FAEE have disordering effects on membranes, exceeding that of ethanol by approximately an order of magnitude;" (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"even low concentrations of FAEE have been demonstrated to inhibit the hepatic enzyme triacylglycerole lipase as well as protein synthesis," (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"Thus, the esterification of free fatty acids in response to an ethanol load may represent part of a toxic cycle (9)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"the carnitine system has also been shown to be involved in a) membrane stabilization and repair, b) the intracellular communication of acyl groups, upon which peroxisomal fatty acid oxidation depends, c) the metabolism of branched chain amino acids or the removal of disruptive acyl CoAs, such as acylcarnitines (10-11)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"In particular, acetyl-L-carnitine is capable of inhibiting alcohol dehydrogenase activity and hence ethanol oxidation (12)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"human volunteers receiving alcohol after treatment with L-carnitine exhibited significantly reduced levels of ethanol in blood associated with increased excretion of acyl-carnitine derivatives (13)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"Therefore, the present investigation was carried out to evaluate the possible interaction between acetyl-L-carnitine and ethanol metabolism in various organs in the rat, and to elucidate the role of enzymes of non-oxidative ethanol metabolism in FAEE formation and their relationship to lipid hydroperoxide production during short-term ethanol intake." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:168)
"Short-term administration of alcohol at a dose of 2g/kg body weight to rats fasted overnight, resulted in a blood ethanol concentration of 56.8 ± 8.3 mmol/l approximately 1 h after ethanol ingestion." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"In the group of rats pretreated with acetyl-L-carnitine, the maximal ethanol concentration in blood was slightly higher than that of animals administered ethanol alone. When the same dose of alcohol was given to L-carnitine pretreated rats, the maximal ethanol concentration in blood was significantly lower (30\%) than that of rats treated with ethanol alone (Fig. 1)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"Pre-treatment with either acetyl-L-carnitine or carnitine, both resulted in a marked decrease in total FAEE levels in all organs studied compared with the group of animals receiving ethanol alone." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"Administration of acetyl-L-carnitine decreased FAEE levels significantly compared to ethanol alone, and this decrease was mainly accounted for by a sparing of unsaturated fatty acids." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"no statistical differences were observed between the L-carnitine treated group and the acetyl-L-carnitine treated group." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"Pre-treatments with either acetyl-L-carnitine or L-carnitine were efficacious in decreasing FAEE levels in kidney, although neither compound was significantly better than the other in this respect." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"Ethanol administration led to a significant increase in the GSH S-transferase activity which (compared to the control group) was approximately 8-fold in the brain, 5-fold in the heart, 2-fold in the kidney and 3-fold in the liver respectively." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"Pre-treatment with acetyl-L-carnitine significantly reduced the ethanol-induced increases of GSH S-transferase activity in the brain, heart and kidney, whereas in the liver no significant differences were observed, compared to the group of animals treated with ethanol alone." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:170)
"was significantly better than L-carnitine pre-treatment at protecting against the ethanol-induced increases in GSH S-transferase activity in all organs except the liver, where L-carnitine had the same efficacy as acetyl-L-carnitine." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:171)
However, pretreatment with acetyl-L-carnitine was significantly better than L-carnitine pre-treatment at protecting against the ethanol-induced increases in GSH S-transferase activity in all organs except the liver, where L-carnitine had the same efficacy as acetyl-L-carnitine. (note on p.171)
 
"Ethanol administration significantly increased the levels of lipid hydroperoxides in all tissues studied." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:171)
"However, pre-treatment with either acetyl-L-carnitine or L-carnitine significantly decreases FAEES activity, although not back to control levels." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:171)
"Interestingly, treatments with L-carnitine or acetyl-L-carnitine, significantly reduced lipid hydroperoxide levels, particularly in brain and heart, but also in the liver and kidney" (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:171)
"Fatty acid ethyl esters have been shown to uncouple oxidative phosphorylation in mitochondria in vivo (9), increase lysosomal fragility (22), and decrease incorporation of thymidine into DNA and methionine into new proteins (23)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:172)
"Moreover, FAEE have been shown to be present in human serum many hours after ethanol ingestion and (24), which suggests that their half-life in tissue may be much longer than that of ethanol itself or its oxidation products." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:172)
"Treatment with acetyl-L-carnitine greatly reduced both FAEE and FAEE synthase activity in all organs and this effect was associated with a significant reduction in the levels of lipid hydroperoxides." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)
"suggesting that the carnitine moiety alone account only partially for the observed effects of acetyl-L-carnitine." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)
"Therefore, it is conceivable that the reduced FAEE formation observed after administration of acetyl-L-carnitine could be a consequence of a reduction of free fatty acid levels, since these are considered the natural substrate for enzymatic FAEE synthesis" (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)
"In addition, acetyl-L-carnitine may protect cells from the injurious effect of accumulating acetaldehyde by attenuating ethanol oxidative metabolism." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)
"Our data support the hypothesis that fatty acid ethyl esters are cytotoxic mediators involved in the production of alcohol-dependent cellular damage (28-29)." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)
"In particular this study strongly suggests a role for the use of acetyl-Lcarnitine as a pharmacological tool in the prevention of alcohol-induced cellular/organ damage." (Calabrese, Scapagnini, Catalano, Dinotta, Bates, Calvani and Stella 2001:173)},
	file = {Calabrese et al_2001_Effects of Acetyl-L-Carnitine on the Formation of Fatty Acid Ethyl Esters in.pdf:/media/truecrypt1/zotero//Neurochemical Research/2001/Calabrese et al_2001_Effects of Acetyl-L-Carnitine on the Formation of Fatty Acid Ethyl Esters in.pdf:application/pdf;Snapshot:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/4AQSMAC6/10.html:text/html}
}

