@article{oliva_protective_2011,
	title = {{PROTECTIVE} {EFFECT} {OF} {QUERCETIN}, {EGCG}, {CATECHIN} {AND} {BETAINE} {AGAINST} {OXIDATIVE} {STRESS} {INDUCED} {BY} {ETHANOL} {IN} {VITRO}},
