	pmid = {7598662},
	keywords = {Adult, Alcohol Drinking, Alcoholic Beverages, Aldehydes, Animals, Bacillus subtilis, ethanol, Female, Fermentation, Humans, Male, Mice, Mice, Inbred Strains},
	pages = {69--79}
