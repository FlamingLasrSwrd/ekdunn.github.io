@article{cao_single_2011,
	title = {Single {Dose} {Administration} of {L}-{Carnitine} {Improves} {Antioxidant} {Activities} in {Healthy} {Subjects}},
	volume = {224},
	doi = {10.1620/tjem.224.209},
	abstract = {L-carnitine has been used as a supplement to treat cardiovascular or liver disease.  However, there has been little information about the effect of L-carnitine on anti-oxidation capability in healthy human subjects.  This study was designed to investigate the correlation between plasma L-carnitine concentration and antioxidant activity.  Liquid L-carnitine (2.0 g) was administered orally as a single dose in 12 healthy subjects.  Plasma concentration of L-carnitine was detected by HPLC.  The baseline concentration of L-carnitine was 39.14 ± 5.65 μmol/L.  After single oral administration, the maximum plasma concentration (Cmax) and area under the curve (AUC0-∞) were 84.7 ± 25.2 μmol/L and 2,676.4 ± 708.3 μmol/L·h, respectively.  The half-life and the time required to reach the Cmax was 60.3 ± 15.0 min and 3.4 ± 0.46 h, respectively.  There was a gradual increase in plasma concentrations of superoxide dismutase (SOD), glutathione peroxidase (GSH-Px), catalase and total antioxidative capacity (T-AOC) in the first 3.5 h following L-carnitine administration.  The plasma concentrations of SOD, GSH-Px, catalase and T-AOC returned to baseline levels within 24 h.  A positive correlation was found between L-carnitine concentration and the antioxidant index of SOD (r = 0.992, P {\textless} 0.01), GSH-Px (r = 0.932, P {\textless} 0.01), catalase (r = 0.972, P {\textless} 0.01) or T-AOC (r = 0.934, P {\textless} 0.01).  In conclusion, L-carnitine increases activities of antioxidant enzymes and the total antioxidant capacity in healthy subjects.  It may be useful as a supplementary therapy for chronic illnesses involving excessive oxidative stress.},
	number = {3},
	journal = {The Tohoku Journal of Experimental Medicine},
	author = {Cao, Yu and Qu, Hai-jun and Li, Ping and Wang, Chun-bo and Wang, Le-xin and Han, Zhi-wu},
	year = {2011},
	keywords = {catalase; glutathione peroxidase; L-carnitine; superoxide dismutase; total antioxidative capacity},
	pages = {209--213},
	file = {Cao_Qu_Li_Wang_Wang_Han_2011_Single Dose Administration of L-Carnitine Improves Antioxidant Activities in.pdf:/media/truecrypt1/zotero//The Tohoku Journal of Experimental Medicine/2011/Cao_Qu_Li_Wang_Wang_Han_2011_Single Dose Administration of L-Carnitine Improves Antioxidant Activities in.pdf:application/pdf;J-Stage - Snapshot:/home/edunn/.mozilla/firefox/ya22vuf6.default/zotero/storage/MWZIZWJR/_article.html:text/html}
}

